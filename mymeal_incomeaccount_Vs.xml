</Ctrls></GroupBox>
<GroupBox Name="GB_isautocode" Multiline="0" cols="xs12 sm6 md4 lg3"><Ctrls>
<CheckBox Name="isautocode" Size="230,20" Text="isautocode" ToolTip="ttp_isautocode"></CheckBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_accounttype" Multiline="0" cols="xs12 sm6 md4 lg3"><Ctrls>
<Label Name="lb_accounttype" Size="60,20" Text="accounttype" Align="Right"/>
<TextBox Name="accounttype" Size="170,20" Text="" Weight="170" ToolTip="ttp_accounttype"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_bank" Multiline="0" cols="xs12 sm6 md4 lg3"><Ctrls>
<Label Name="lb_bank" Size="60,20" Text="bank" Align="Right"/>
<ComboBox Name="bank" Size="170,100" Text="" Weight="170"  ToolTip="ttp_bank"></ComboBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_bankaccount" Multiline="0" cols="xs12 sm6 md4 lg3"><Ctrls>
<Label Name="lb_bankaccount" Size="60,20" Text="bankaccount" Align="Right"/>
<TextBox Name="bankaccount" Size="170,20" Text="" Weight="170" ToolTip="ttp_bankaccount"></TextBox>
