<GroupBox Name="GBDEDITmymeal_businesstime"><Ctrls>
<GroupBox cols="xs12 sm6 md4 lg3" Multiline="0" Name="GB_name"><Ctrls>
<Label Align="Right" Text="name" Name="lb_name" Size="60,20"/>
<TextBox Weight="170" Text="" ToolTip="ttp_name" Name="name" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4 lg3" Multiline="0" Name="GB_starttime"><Ctrls>
<Label Align="Right" Text="starttime" Name="lb_starttime" Size="60,20"/>
<DateTime Weight="170" Text="" ToolTip="ttp_starttime" Name="starttime" Size="170,20" Format="yyyy-MM-dd HH:mm:ss"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4 lg3" Multiline="0" Name="GB_endtime"><Ctrls>
<Label Align="Right" Text="endtime" Name="lb_endtime" Size="60,20"/>
<DateTime Weight="170" Text="" ToolTip="ttp_endtime" Name="endtime" Size="170,20" Format="yyyy-MM-dd HH:mm:ss"/>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox cols="xs12 sm6 md4 lg3" Multiline="0" Name="GB_displayorder"><Ctrls>
<Label Align="Right" Text="displayorder" Name="lb_displayorder" Size="60,20"/>
<TextBox Weight="170" Text="" ToolTip="ttp_displayorder" Name="displayorder" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4 lg3" Multiline="0" overlap="pre" Name="GB_isdelete" Visible="false"><Ctrls>
<CheckBox Text="isdelete" ToolTip="ttp_isdelete" overlap="pre" Name="isdelete" readonly="true" Size="230,20" Visible="false"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4 lg3" Multiline="0" overlap="pre" Name="GB_timeid" Visible="false"><Ctrls>
<Label Align="Right" Text="timeid" overlap="pre" Name="lb_timeid" readonly="true" Size="60,20" Visible="false"/>
<TextBox Weight="170" Text="" ToolTip="ttp_timeid" overlap="pre" Name="timeid" readonly="true" Size="170,20" Visible="false"><OnEnChange><if cond="'%{timeid}'"><SetVisible name="insertmymeal_businesstime" Visible="0"/><SetVisible name="modifymymeal_businesstime" Visible="1"/><else/><SetVisible name="insertmymeal_businesstime" Visible="1"/><SetVisible name="modifymymeal_businesstime" Visible="0"/></if></OnEnChange></TextBox>
</Ctrls></GroupBox>
</Ctrls></GroupBox>