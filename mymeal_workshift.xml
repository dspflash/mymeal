<BR AutoHeight="1"/><GroupBox  Name="GBOPmymeal_workshift" Size="100,100" cols="xs12 sm12 md12 lg12"><Ctrls>
<Button Name="insertmymeal_workshift" Size="50,20" Text="create" ToolTip="ttp_createmymeal_workshift">
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>mymeal.mymeal_workshift</DbTable>
<Data>docno,entrytypeid[entrytypeid],docdate,possiteid[possiteid],operatorid[userloginid],acceptid[userloginid],incomeamount,icsaveamount,diningamount,discountamount,hangupamount,handtime,handinamount,downamount,balanceamount,status,id_corp</Data>
<queryFormat>insert into %{table} %{data}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchmymeal_workshift" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Name="modifymymeal_workshift" Size="50,20" Text="modify" ToolTip="ttp_modifymymeal_workshift">
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>mymeal.mymeal_workshift</DbTable>
<Data>docno,entrytypeid[entrytypeid],docdate,possiteid[possiteid],operatorid[userloginid],acceptid[userloginid],incomeamount,icsaveamount,diningamount,discountamount,hangupamount,handtime,handinamount,downamount,balanceamount,status,id_corp</Data>
<Where>shiftid='%{mymeal_workshiftLst[shiftid]}'</Where>
<queryFormat>update %{table} set %{data} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchmymeal_workshift" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Name="deletemymeal_workshift" Size="50,20" Text="delete" ToolTip="ttp_deletemymeal_workshift">
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>mymeal.mymeal_workshift</DbTable>
<Where>shiftid='%{mymeal_workshiftLst[shiftid]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchmymeal_workshift" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Name="searchmymeal_workshift" Size="50,20" Text="refresh" ToolTip="ttp_refreshmymeal_workshift">
			  <OnClick><submit next="mymeal_workshiftLst"/>	</OnClick>
</Button>
<TAB/><Button Name="FPmymeal_workshiftLst" Size="40,20" Text="firstpage" OnClick="(mymeal_workshiftLst)GetPage(First)"></Button>
<Button Name="PREmymeal_workshiftLst" Size="40,20" Text="prepage" OnClick="(mymeal_workshiftLst)GetPage(Pre)"></Button>
<Button Name="NPmymeal_workshiftLst" Size="40,20" Text="nextpage" OnClick="(mymeal_workshiftLst)GetPage(Next)"></Button>
</Ctrls></GroupBox>
<BR/><GroupBox Name="GBDVmymeal_workshift" cols="xs12 sm12 md12 lg12"><Ctrls>
<ListCtrl Name="mymeal_workshiftLst" ToolTip="ttp_mymeal_workshift" Size="700,420" PageSize="10" loadedclick="0" Heading="docno,100;entrytypeid,120;docdate,150;possiteid,120;operatorid,120;acceptid,120;incomeamount,90;icsaveamount,90;diningamount,90;discountamount,90;hangupamount,90;handtime,150;handinamount,90;downamount,90;balanceamount,90;status,120;id_corp,55;shiftid,100;">
	<DbDetails fcsm="1">
<DbTable>mymeal.mymeal_workshift</DbTable>
<Index>shiftid='%{mymeal_workshiftLst[shiftid]}'</Index>
<Data>docno,entrytypeid,docdate,possiteid,operatorid,acceptid,incomeamount,icsaveamount,diningamount,discountamount,hangupamount,handtime,handinamount,downamount,balanceamount,status,id_corp,shiftid</Data>
<Order>shiftid</Order></DbDetails>
<OnClick><assign name="entrytypeid[entrytypeid]" expr="%{mymeal_workshiftLst[entrytypeid]}" function="SetSel"/><assign name="possiteid[possiteid]" expr="%{mymeal_workshiftLst[possiteid]}" function="SetSel"/><assign name="operatorid[userloginid]" expr="%{mymeal_workshiftLst[operatorid]}" function="SetSel"/><assign name="acceptid[userloginid]" expr="%{mymeal_workshiftLst[acceptid]}" function="SetSel"/></OnClick>
</ListCtrl>
</Ctrls></GroupBox></Ctrls></GroupBox>
</Ctrls>
	<OnChanged></OnChanged>
	<OnActivate></OnActivate>
	</WndTap>
