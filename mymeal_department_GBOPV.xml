	<OnClick>
	<DbDetails fcsm="1">
<DbTable>mymeal.mymeal_department</DbTable>
<Where>departmentid='%{mymeal_departmentLst[departmentid]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>
