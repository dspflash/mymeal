<GroupBox Name="GBDEDITmymeal_iccard"><Ctrls>
<GroupBox cols="xs12 sm6 md4 lg3" Multiline="0" Name="GB_memberid"><Ctrls>
<Label Align="Right" Text="memberid" onload="init" Name="lb_memberid" Size="60,20"/>
