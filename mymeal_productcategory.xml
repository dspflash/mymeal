<Button Name="insertmymeal_productcategory" Size="50,20" Text="create" ToolTip="ttp_insertmymeal_productcategory">
	<OnClick>
	<DbDetails fcsm="1"><DbTable>mymeal.mymeal_productcategory</DbTable><Data>name,code,producttype,parentid,backprinttype,backprinter,printwidth,printtitle,isprinttotal,printtotalnumber,printtotalfont,isprintpart,printpartnumber,printpartfont,iscutpaper,isprintnumberexpend,isdelete,deletetime,images,id_corp</Data><queryFormat>insert into %{table} %{data}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchmymeal_productcategory" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Name="modifymymeal_productcategory" Size="50,20" Text="modify" ToolTip="ttp_modifymymeal_productcategory">
	<OnClick>
	<DbDetails fcsm="1"><DbTable>mymeal.mymeal_productcategory</DbTable><Data>name,code,producttype,parentid,backprinttype,backprinter,printwidth,printtitle,isprinttotal,printtotalnumber,printtotalfont,isprintpart,printpartnumber,printpartfont,iscutpaper,isprintnumberexpend,isdelete,deletetime,images,id_corp</Data><Where>categoryid='%{mymeal_productcategoryLst[categoryid]}'</Where><queryFormat>update %{table} set %{data} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchmymeal_productcategory" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Name="deletemymeal_productcategory" Size="50,20" Text="delete" ToolTip="ttp_deletemymeal_productcategory">
	<OnClick>
	<DbDetails fcsm="1"><DbTable>mymeal.mymeal_productcategory</DbTable><Where>categoryid='%{mymeal_productcategoryLst[categoryid]}'</Where><queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchmymeal_productcategory" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Name="searchmymeal_productcategory" Size="50,20" Text="refresh" ToolTip="ttp_refreshmymeal_productcategory">
			  <OnClick><submit next="mymeal_productcategoryLst"/>	</OnClick>
</Button>
<TAB/><Button Name="FPmymeal_productcategoryLst" Size="40,20" Text="firstpage" OnClick="(mymeal_productcategoryLst)GetPage(First)"></Button>
<Button Name="PREmymeal_productcategoryLst" Size="40,20" Text="prepage" OnClick="(mymeal_productcategoryLst)GetPage(Pre)"></Button>
<Button Name="NPmymeal_productcategoryLst" Size="40,20" Text="nextpage" OnClick="(mymeal_productcategoryLst)GetPage(Next)"></Button>
</Ctrls></GroupBox>
<BR/><GroupBox Name="GBDVmymeal_productcategory" cols="xs12 sm12 md12 lg12"><Ctrls>
<ListCtrl Name="mymeal_productcategoryLst" ToolTip="ttp_mymeal_productcategory" Size="700,420" PageSize="10" loadedclick="0" Heading="name,320;code,160;producttype,120;parentid,55;backprinttype,100;backprinter,20;printwidth,160;printtitle,320;isprinttotal,60;printtotalnumber,55;printtotalfont,160;isprintpart,60;printpartnumber,55;printpartfont,160;iscutpaper,60;isprintnumberexpend,60;isdelete,60;deletetime,150;images,250;id_corp,55;categoryid,55;" onload="init">
	<DbDetails fcsm="1"><DbTable>mymeal.mymeal_productcategory</DbTable><Index>categoryid='%{mymeal_productcategoryLst[categoryid]}'</Index><Data>name,code,producttype,parentid,backprinttype,backprinter,printwidth,printtitle,isprinttotal,printtotalnumber,printtotalfont,isprintpart,printpartnumber,printpartfont,iscutpaper,isprintnumberexpend,isdelete,deletetime,images,id_corp,categoryid</Data><Order>categoryid</Order></DbDetails><DbDetails fcsm="1"><queryFormat>select %{data} from %{dbtable} limit %{StartRow},%{PgSize}</queryFormat></DbDetails></ListCtrl>
</Ctrls></GroupBox></Ctrls></GroupBox>
</Ctrls>
	<OnChanged></OnChanged>
	<OnActivate></OnActivate>
	</WndTap>
